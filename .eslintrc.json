{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-prototype-builtins": "off",
    "no-undef": "off",
    "no-empty": "off",
    "no-unused-vars": "off",
    "no-fallthrough": "off",
    "no-sparse-arrays": "off",
    "no-extra-semi": "off",
    "no-unexpected-multiline": "off",
    "no-unsafe-finally": "off",
    "no-cond-assign": "off",
    "no-useless-escape": "off",
    "no-self-assign": "off",
    "no-func-assign": "off",
    "no-control-regex": "off",
    "getter-return": "off"
  }
}
